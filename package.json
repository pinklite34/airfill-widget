{
  "name": "@bitrefill/airfill-widget",
  "version": "4.5.2",
  "description": "Airfill Embeddable Refill Widget",
  "main": "dist/index.js",
  "engines": {
    "node": "^8.2.0",
    "yarn": "^1.1.0",
    "npm": "^5.6.0"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "copy": "cp -f ./src/index.html ./dist",
    "prebuild": "npm run clean && npm run copy",
    "build:standalone": "webpack --config config/standalone.js --json > stats.json",
    "build:module": "webpack --config config/dist.js --json > stats.json",
    "build": "npm run build:translations && npm run prebuild && npm run build:module && npm run build:standalone",
    "build:watch": "npm run prebuild && npm run build:module -- --mode=development -w",
    "build:translations": "NODE_ENV=production node scripts/translations.js",
    "dist": "webpack --env=dist --json > stats.json",
    "standalone": "npm run copy && webpack --env=standalone --json > stats.json",
    "lint": "tslint -p .",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "start": "webpack-dev-server --config config/dev.js --open",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "webpack-stats": "webpack-bundle-analyzer stats.json",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "heroku": "node config/herokuServer.js",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [],
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "dependencies": {
    "@alienfast/i18next-loader": "^1.0.16",
    "@bitrefill/react-pusher": "^2.3.2",
    "color": "^3.0.0",
    "core-js": "^2.5.7",
    "downshift": "^1.31.7",
    "emotion": "^9.2.6",
    "emotion-theming": "^9.2.6",
    "ethjs": "^0.4.0",
    "express": "^4.16.3",
    "fetch-ponyfill": "^6.0.1",
    "flag-icons": "^1.0.2",
    "fs-extra": "^7.0.0",
    "history": "^4.7.2",
    "i18next": "^11.5.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "input-format": "^0.2.4",
    "libphonenumber-js": "^1.4.2",
    "locize-cli": "^3.12.0",
    "material-ui": "1.0.0-beta.27",
    "prop-types": "^15.5.8",
    "qrious": "^4.0.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.6",
    "react-i18next": "^7.10.1",
    "react-loadable": "^5.4.0",
    "react-media": "^1.8.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-tiny-virtual-list": "^2.2.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.6.1",
    "redux-persist": "^4.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/history": "^4.7.0",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "eslint-config-bitrefill": "^1.1.1",
    "husky": "^0.14.3",
    "jest": "23.5.0",
    "lint-staged": "^7.2.2",
    "minimist": "^1.2.0",
    "prettier": "^1.14.2",
    "react-addons-test-utils": "^15.0.0",
    "react-test-renderer": "16.4.2",
    "svg-react-loader": "^0.4.5",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "unused-files-webpack-plugin": "^3.4.0",
    "url-loader": "^1.0.1",
    "webpack": "4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts|js)$",
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrefill/airfill-widget.git"
  },
  "bugs": {
    "url": "https://github.com/bitrefill/airfill-widget/issues"
  },
  "homepage": "https://github.com/bitrefill/airfill-widget#readme",
  "license": "MIT"
}
